var parcelHelpers = require("@parcel/transformer-js/src/esmodule-helpers.js");
parcelHelpers.defineInteropFlag(exports);
parcelHelpers.export(exports, "getJSON", ()=>getJSON
);
// For functions used many times
const timeout = function(s) {
    return new Promise(function(_, reject) {
        setTimeout(()=>{
            reject(new Error(`ğŸ’¥ğŸ’¥ğŸ’¥ Request took took long: ${s}s`));
        }, s);
    });
};
// getJSON and Error handling
const getJSON = async function(url) {
    try {
        const timeoutPromsie = timeout(1);
        const res = await Promise.race([
            timeoutPromsie,
            fetch(url)
        ]);
        const data = await res.json();
        // Check for valid url
        if (!res.ok) throw new Error(`ğŸ’¥ğŸ’¥ğŸ’¥ ${data.message} ${res.status}`);
        // Destructure for recipe object and return
        return data.data.recipe;
    } catch (err) {
        console.error(err);
        throw err;
    }
};
