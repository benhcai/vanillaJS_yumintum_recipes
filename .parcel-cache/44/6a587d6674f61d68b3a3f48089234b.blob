var parcelHelpers = require("@parcel/transformer-js/src/esmodule-helpers.js");
parcelHelpers.defineInteropFlag(exports);
parcelHelpers.export(exports, "getJSON", ()=>getJSON
);
// For functions used many times
const timeout = function(s) {
    return new Promise(function(resolve, reject) {
        setTimeout(function() {
            reject(new Error(`Request took too long! Timeout after ${s} second`));
        }, s);
    });
};
// getJSON and Error handling
const getJSON = async function(url) {
    try {
        const fetchUrl = fetch(url);
        const res = await Promise.race([
            timeout(5),
            fetchUrl
        ]);
        const data = await res.json();
        // Check for valid url
        if (!res.ok) throw new Error(`ðŸ’¥ !res.ok - ${data.message} ${res.status}`);
        // Destructure for recipe object and return
        return data.data.recipe;
    } catch (err) {
        console.error("catched: ", err);
        throw err;
    }
};
const testTimeout = async function() {
    try {
        const res = await Promise.race([
            timeout(1),
            timeout(15)
        ]);
    } catch (err) {
        console.log("testTime:", err);
    }
};
testTimeout();
